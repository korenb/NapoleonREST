    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch API Notes",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "api-build",
            "program": "${workspaceFolder}/NapoleonNotes/bin/Debug/netcoreapp2.2/NapoleonNotes.dll",
            "args": [],
            "cwd": "${workspaceFolder}/NapoleonNotes",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": false
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        }
    ]
}